{
    "name": "trellis-ldp/static-ldp",
    "type": "project",
    "description": "LDP Server for static assets",
    "keywords": ["ldp"],
    "homepage": "https://github.com/trellis-ldp/static-ldp",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.6.0",
        "silex/silex": "^2.0",
        "symfony/config": "^3.4",
        "symfony/yaml": "^3.4",
        "easyrdf/easyrdf": "^0.9.1",
        "ml/json-ld": "^1.0",
        "twig/twig": "^1.33"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/browser-kit": "^3.4",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {"Trellis\\StaticLdp\\": "src/"}
    },
    "scripts": {
        "check": "phpcs --standard=PSR2 src test index.php",
        "test": [
            "@check",
            "phpunit"
        ]
    }
}
